<!-- Youtube -->
div(class="youtube-search")
	div(class="form-group")
		<label class="label-control warning">maximum number of search results
		<span class="warningText">Careful! Every 50 count is ONE request, and you only have 180 requests allowed over 15 minutes window</span></label>
		div(class="range range-node")
			input(type="range" min="100" max="1000" value="500" step="50" id="youtube-count" onchange="ytrangeNode.value=value")
			<output id="ytrangeNode">500</output>
	div(class="form-group")
		<label class="label-control" style="display:block;">Order</label>
		<label class="radio-inline"><input type="radio" name="order" value="date" checked>date</label>
		<label class="radio-inline"><input type="radio" name="order" value="rating">rating</label>
		<label class="radio-inline"><input type="radio" name="order" value="relevance">relevance</label>
		<label class="radio-inline"><input type="radio" name="order" value="title">title</label>
		<label class="radio-inline"><input type="radio" name="order" value="videoCount">videoCount</label>
		<label class="radio-inline"><input type="radio" name="order" value="viewCount">viewCount</label>
	div(class="form-group")
		<label class="label-control" style="display:block;">Type</label>
		<label class="radio-inline"><input type="radio" name="type" value="channel" checked>channel</label>
		<label class="radio-inline"><input type="radio" name="type" value="playlist">playlist</label>
		<label class="radio-inline"><input type="radio" name="type" value="video">video</label>
	div(class="form-group")
		<label class=""><i class="fas fa-map-marker-alt"></i>&nbsp;GEOCODE ENABLE
		<input type="checkbox" id="geocode" value="geocode">
		</label>
	div(class="form-group geocode")
		div(class="col-md-5" style="display:inline-block;")
			label Latitude
			input(class="form-control",type="number",step="0.01",id="lat")
		div(class="col-md-5" style="display:inline-block;")
			label Longitude
			input(class="form-control",type="number",step="0.01",id="lon")
		div(class="col-md-2" style="display:inline-block;")
			label radius(mile)
			input(class="form-control",type="number",step="0.01",id="radius")
		div(class="col-md-12")
			iframe(id="openLayerMap" src="openLayerMap/geolocation.html")
